
PLATFORMID=12889
PLATFORMOS=dryos

ROMBASEADDR=0xFF000000
MAXRAMADDR=0x0fffffff

EXMEM_HEAP_SKIP=0x679800	#(0x50000000-0x4F986800) from sub_FF095DA8 = size of video buffers in EXMEM area 
EXMEM_BUFFER_SIZE=0x100000	# Amount of EXMEM memory to allocate for CHDK; 40000=256KB, 80000=512KB, A0000=640kb, C0000=768KB, 100000=1MB

ifdef OPT_CHDK_IN_EXMEM
MEMISOSTART=0xF8867E0       #(MAXRAMADDR +1- EXMEM_HEAP_SKIP - EXMEM_BUFFER_SIZE - 32)
else
MEMISOSTART=0x1B3DD4        # original non-exmem value
endif

KEYSYS=d4c
NEED_ENCODED_DISKBOOT=9
PLATFORMOSVER=51
