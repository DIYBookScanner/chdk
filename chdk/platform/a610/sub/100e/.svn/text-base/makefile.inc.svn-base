#0x30FD
PLATFORMID=12541

PLATFORMOS=vxworks

ROMBASEADDR=0xffc00000

MAXRAMADDR=0x01FFFFFF # 32MB RAM
EXMEM_BUFFER_SIZE=0x800000 # Amount of EXMEM memory to allocate for CHDK = 8MB, on change update MEMISOSTART
EXMEM_HEAP_SKIP=0x0 # no skipping needed

ifdef OPT_CHDK_IN_EXMEM
MEMISOSTART=0x1800000  # MAXRAMADDR+1 - EXMEM_HEAP_SKIP - EXMEM_BUFFER_SIZE (without -32 by VxWorks)
else
MEMISOSTART=0xA0B70  # original non-exmem value
endif

